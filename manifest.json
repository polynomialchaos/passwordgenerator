{
    "name": "Password generator",
    "version": "1.1",
    "manifest_version": 2,
    "description": "An edge extension to generate secure passwords.",
    "icons": {
        "16": "icons/pwd_gen_16.png",
        "32": "icons/pwd_gen_32.png",
        "48": "icons/pwd_gen_48.png",
        "128": "icons/pwd_gen_128.png"
    },
    "permissions": [
        "storage"
    ],
    "browser_action": {
        "default_popup": "popup/popup.html"
    }
}
